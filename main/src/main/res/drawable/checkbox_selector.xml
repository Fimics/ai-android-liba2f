<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 选中状态 -->
    <item android:state_checked="true"
        android:drawable="@drawable/ic_checkbox_checked" /> <!-- 替换为你的选中图标 -->

    <!-- 未选中状态 -->
    <item android:state_checked="false"
        android:drawable="@drawable/ic_checkbox_unchecked" /> <!-- 替换为你的未选中图标 -->
</selector>
